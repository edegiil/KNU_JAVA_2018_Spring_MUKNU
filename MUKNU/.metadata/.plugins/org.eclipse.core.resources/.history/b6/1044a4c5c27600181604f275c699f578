package application.view;

import javafx.beans.property.StringProperty;

public class RestaurantList {
	private StringProperty name;
	private StringProperty gate;
	private StringProperty price;
	private StringProperty menu;
	private StringProperty tel;
	
	public RestaurantList(String name, String gate, String price, String menu, String tel) {
		this.name = name;
		this.gate = gate;
		this.price = price;
		this.menu = menu;
		this.tel = tel;
	}

	public StringProperty nameProperty() {
		return this.name;
	}

	public StringProperty gateProperty() {
		return this.gate;
	}
	
	public StringProperty priceProperty() {
		return this.price;
	}
	
	public StringProperty menuProperty() {
		return this.menu;
	}

	public StringProperty telProperty() {
		return this.tel;
	}
}
